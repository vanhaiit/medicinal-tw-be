{
    "name": "orasis-be",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\"",
        "dev": "cp .env.development .env && nest start --watch",
        "debug": "nest start --debug --watch",
        "start": "nest start",
        "start:prod": "node dist/main",
        "prepare": "husky install",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --max-warnings=0",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ./src/data-source.ts",
        "migration:create": "cd src/migrations && npx typeorm-ts-node-commonjs migration:create",
        "migration:run": "npm run typeorm migration:run",
        "migration:revert": "npm run typeorm migration:revert",
        "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
        "console": "node dist/console.js",
        "new": "hygen new",
        "generate:module": "nest g resource"
    },
    "dependencies": {
        "@mikro-orm/postgresql": "^6.3.1",
        "@nestjs/cache-manager": "^2.2.0",
        "@nestjs/common": "^10.2.5",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.2.5",
        "@nestjs/jwt": "^10.1.1",
        "@nestjs/passport": "^10.0.2",
        "@nestjs/platform-express": "^10.4.1",
        "@nestjs/platform-socket.io": "^10.3.0",
        "@nestjs/schedule": "^4.0.1",
        "@nestjs/serve-static": "^4.0.2",
        "@nestjs/swagger": "^7.1.11",
        "@nestjs/terminus": "^10.2.0",
        "@nestjs/typeorm": "^10.0.0",
        "@nestjs/websockets": "^10.3.0",
        "@socket.io/redis-adapter": "^8.2.1",
        "@socket.io/redis-emitter": "^5.1.0",
        "axios": "^1.7.2",
        "bcrypt": "^5.1.1",
        "bull": "^4.12.2",
        "cache-manager": "4.1.0",
        "cache-manager-redis-store": "2.0.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "config": "^3.3.9",
        "dayjs": "^1.11.9",
        "dotenv": "^16.0.1",
        "fs": "^0.0.1-security",
        "handlebars": "^4.7.8",
        "helmet": "^5.1.1",
        "ipfs-http-client": "^60.0.1",
        "jwt-decode": "^4.0.0",
        "lint-staged": "^14.0.1",
        "log4js": "6.9.1",
        "nestjs-console": "^9.0.0",
        "node-gyp": "^10.1.0",
        "nodemailer": "^6.9.14",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.11.5",
        "redis": "^4.6.12",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "serve-static": "^1.15.0",
        "socket.io": "^4.7.4",
        "typeorm": "^0.3.17",
        "typeorm-transactional": "^0.4.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@handfish/hygen": "^6.1.6",
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@trivago/prettier-plugin-sort-imports": "^3.3.0",
        "@types/cache-manager": "^4.0.6",
        "@types/config": "^3.3.3",
        "@types/express": "^4.17.13",
        "@types/ioredis": "^5.0.0",
        "@types/jest": "28.1.4",
        "@types/multer": "^1.4.11",
        "@types/node": "^20.6.2",
        "@types/node-fetch": "^2.6.11",
        "@types/nodemailer": "^6.4.15",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/stream-buffers": "^3.0.7",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^8.0.3",
        "jest": "28.1.2",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "28.0.5",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "4.0.0",
        "typescript": "^5.2.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "lint-staged": {
        "*.ts": [
            "prettier --write",
            "eslint --fix"
        ]
    }
}